<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:dotnet_music_player_app.Resources.ViewModel"
                    xmlns:view="clr-namespace:dotnet_music_player_app.Resources.View">

    <DataTemplate DataType="{x:Type vm:SongPage}">
        <view:SongPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PlayLists}">
        <view:PlayLists/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:MostPlayed}">
        <view:MostPlayed/>
    </DataTemplate>



</ResourceDictionary>